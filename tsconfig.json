{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@components": [
        "src/app/components/index"
      ],
      "@modules/*": [
        "src/app/modules/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "@shared": [
        "src/app/shared/index"
      ],
      "@base-components": [
        "src/core/base-components/index"
      ],
      "@constants": [
        "src/core/constants/index"
      ],
      "@enums": [
        "src/core/enums/index"
      ],
      "@guards": [
        "src/core/guards/index"
      ],
      "@interceptors": [
        "src/core/interceptors/index"
      ],
      "@models": [
        "src/core/models/index"
      ],
      "@services": [
        "src/core/services/index"
      ],
      "@utils": [
        "src/core/utils/index"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
